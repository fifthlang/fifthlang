@using Fifth.CodeGeneration;
@using fifth.metamodel.metadata.il;
@using fifth.metamodel.metadata;
@inherits BaseTemplatePage<ClassDefinition>

.class public @Model.Name extends [System.Runtime]System.Object
{
    @foreach(var f in Model.Fields)
    {
        await IncludeAsync("field.cshtml", f);
    }

    @foreach (var p in Model.Properties)
    {
        await IncludeAsync("property.cshtml", p);}
    }

    @foreach (var m in Model.Methods)
    {
        await IncludeAsync("method.cshtml", m);
    }

}
